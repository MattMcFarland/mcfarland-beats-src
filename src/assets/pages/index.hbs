{{> header itemtype="http://schema.org/CollectionPage" title=index.title description=index.description}}
<article class="uk-article">
    <header>
        <h1 itemprop="name" class="uk-article-title">{{index.title}}</h1>
        <p itemprop="description" class="uk-article-lead">{{index.description}}</p>
    </header>
    <section itemscope itemtype="http://schema.org/MusicPlaylist" >
        <h2 itemprop="name">Tracks</h2>
        <meta itemprop="numTracks" content="61"/>
        <div class="uk-overflow-container">
            <table class="uk-table">
                <thead>
                <tr>
                    <th>Genre</th><th>Name</th><th>Duration</th>
                </tr>
                </thead>
                {{#each tracks.tracks}}
                    {{> trackrow}}
                {{/each}}
            </table>
        </div>
    </section>
</article>
{{> footer }}